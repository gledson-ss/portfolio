{
  "extends": [
    "some-other-config-you-use",
    "prettier",
    "prettier/@typescript-eslint",
    "prettier/babel",
    "prettier/flowtype",
    "prettier/react",
    "prettier/standard",
    "prettier/unicorn",
    "prettier/vue"
  ],
  "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "tsx":true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
  "plugins": [
    "prettier",
    "react",
    "@typescript-eslint"
  ],

  "rules": {
    "prettier/prettier": "error"
    
  }
}